					phandle = <0x39>;
				};

				cpu_hot {
					temperature = <0x15f90>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x3a>;
				};

				cpu_very_hot {
					temperature = <0x186a0>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x3b>;
				};

				cpu_crit {
					temperature = <0x19a28>;
					hysteresis = <0x7d0>;
					type = "critical";
				};
			};

			cooling-maps {

				cpu-hot-limit {
					trip = <0x38>;
					cooling-device = <0x33 0xffffffff 0xffffffff 0x34 0xffffffff 0xffffffff 0x35 0xffffffff 0xffffffff 0x36 0xffffffff 0xffffffff>;
				};

				cpu_warm_limit_cpu {
					trip = <0x39>;
					cooling-device = <0x33 0xffffffff 0x01>;
				};

				cpu_hot_limit_cpu {
					trip = <0x3a>;
					cooling-device = <0x33 0x02 0x03>;
				};

				cpu_very_hot_limit_cpu {
					trip = <0x3b>;
					cooling-device = <0x33 0x05 0xffffffff>;
				};
			};
		};
	};

